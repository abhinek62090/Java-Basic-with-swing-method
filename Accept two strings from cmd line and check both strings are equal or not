class test
{
	public static void main(String []a)
	{
		int i;
		if(a.length==2)
			{
				if(a[0].equals(a[1]))
				{
					System.out.println("Both are equal");
				}
				else
				{
					System.out.println("Both are not equal");
				}
			}
			else
			{
				System.out.println("Only two strings are allowed");
			}
		}
	
}

